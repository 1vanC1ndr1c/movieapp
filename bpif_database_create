create table movie_category (movie_id bigint not null, category_list_id bigint not null) engine=MyISAM
create table movies (id bigint not null auto_increment, movie_description varchar(255), name varchar(255), runtime bigint, release_date bigint, primary key (id)) engine=MyISAM
create table movies_directors (movie_id bigint not null, directors_id bigint not null) engine=MyISAM
create table movies_stars (movie_id bigint not null, stars_id bigint not null) engine=MyISAM
create table movies_writers (movie_id bigint not null, writers_id bigint not null) engine=MyISAM
create table person (id bigint not null auto_increment, bio varchar(255), birth_place varchar(255), name varchar(255), birth_date bigint, roles bigint, primary key (id)) engine=MyISAM
create table person_filmography (person_id bigint not null, filmography_id bigint not null) engine=MyISAM
